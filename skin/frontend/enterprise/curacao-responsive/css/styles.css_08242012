/*================================================================================ */
/* 1. Basics & Buttons												Magify.com
/*================================================================================ */

* { 
	-moz-box-sizing: border-box; 
	-webkit-box-sizing: border-box; 
	box-sizing: border-box; 
	font-weight: normal;
	font: 12px/16px 'AkzidenzGrotesk-Regular', "HelveticaNeue", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
img {
	max-width: 100%;
}
body{ background: #fff; font: 14px/21px 'AkzidenzGrotesk-Regular', 'HelveticaNeue', 'Helvetica Neue', Helvetica, Arial, sans-serif; }

	.button,
	button,
	input[type="submit"],
	input[type="reset"],
	input[type="button"] {
		background: url("../images/icons.png") no-repeat scroll 0 -64px transparent;
		border: 0 none;
		color: #FFFFFF;
		float: left;
		font-family: 'AkzidenzGrotesk-Bold';
		font-size: 14px;
		font-weight: normal;
		margin: 0;
		padding: 6px 16px;
		text-shadow: none;
		text-transform: capitalize;
	}
	.button span span {
		font-family: 'AkzidenzGrotesk-Bold';
		font-weight: normal;
	}

	.button:hover,
	button:hover,
	input[type="submit"]:hover,
	input[type="reset"]:hover,
	input[type="button"]:hover {
		text-shadow: none;
	}

	.button:active,
	button:active,
	input[type="submit"]:active,
	input[type="reset"]:active,
	input[type="button"]:active {
	background: #d86939;
	}
	.four.columns.leftcol{ float: left;}
	.twelve.columns.col2l.content{ float: right; }
	.twelve.columns.col2r.content{ float: left;} 
	.four.columns.rightcol{ float: right !important;}
	
	.right { float: right; }
	.hidden { display: none; visibility: hidden; }

/*================================================================================ */
/* 2. Typography													Magify.com
/*================================================================================ */

	@font-face {
	    font-family: 'AkzidenzGrotesk-Bold';
	    src: url('../fonts/berthold_akzidenz_grotesk_bold.eot');
	    src: url('../fonts/berthold_akzidenz_grotesk_bold.eot?#iefix') format('embedded-opentype'),
	         url('../fonts/berthold_akzidenz_grotesk_bold.woff') format('woff'),
	         url('../fonts/berthold_akzidenz_grotesk_bold.ttf') format('truetype'),
	         url('../fonts/berthold_akzidenz_grotesk_bold.svg#berthold_akzidenz_grotesk_bBd') format('svg');
	    font-weight: normal;
	    font-style: normal;
	
	}
	
	
	
	
	@font-face {
	    font-family: 'AkzidenzGrotesk-Regular';
	    src: url('../fonts/berthold_akzidenz_grotesk_regular.eot');
	    src: url('../fonts/berthold_akzidenz_grotesk_regular.eot?#iefix') format('embedded-opentype'),
	         url('../fonts/berthold_akzidenz_grotesk_regular.woff') format('woff'),
	         url('../fonts/berthold_akzidenz_grotesk_regular.ttf') format('truetype'),
	         url('../fonts/berthold_akzidenz_grotesk_regular.svg#berthold_akzidenz_grotesk_bRg') format('svg');
	    font-weight: normal;
	    font-style: normal;
	
	}
	
	
	h1, h2, h3, h4, h5, h6 {
		color: #444;
		font-family: "AkzidenzGrotesk-Bold", Helvetica, Arial, Verdana;
		font-weight: normal;
	}
	h1 a, h2 a, h3 a, h4 a, h5 a, h6 a { font-weight: normal; }
	h1 {
		font-size: 28px;
	    font-weight: normal;
	    line-height: 48px;
	    margin-bottom: 18px;
	}
	h2 { font-size: 24px; line-height: 42px; margin-bottom: 12px; }
	h3 { font-size: 18px; line-height: 36px; margin-bottom: 6px; }
	h4 { font-size: 16px; line-height: 30px; margin-bottom: 0; }
	h5 { font-size: 14px; line-height: 24px; }
	h6 { font-size: 12px; line-height: 18px; }
	h2.legend{ font-size: 16px; text-transform:uppercase; line-height: 18px; }
	
	a, a:visited {
		color: #fe4b05;
		text-decoration: none;
		cursor: pointer;
	}

	strong { font-weight: normal; font-family: 'AkzidenzGrotesk-Bold'; }
      
/*================================================================================ */
/* 3. Header														Magify.com
/*================================================================================ */
	header{
		margin: 0;
		background: url("../images/bg_page.png") repeat-x scroll 0 bottom #FFFFFF;
		padding-top: 8px;
		border-bottom: 1px solid #dadada;
		z-index: 9;
		position: relative;
	}
	.container { z-index: 0; }
	
	.logo a{
		display: block;
		background: url(../images/logo.gif) no-repeat;
		height: 52px;
		/* http://nicolasgallagher.com/another-css-image-replacement-technique/ */
		font: 0/0 a;
		text-shadow: none;
		color: transparent;
		width: 100%;
	}
	h1.logo{
		margin: -4px 0 24px;
	}
	
	header ul.links { font-size: 12px; }
	
	header .right ul.links {
		border-radius: 0 0 6px 6px;
		float: right;
		margin: -8px 0 8px;
		padding: 6px 8px 0;
	}
	header .right ul.links li { float: right; margin-bottom: 6px; }
	
	header .right ul.links li.first {}
	header .right ul.links li.last {}
	
	header ul.links li a {
		color: #444444;
		font-weight: normal;
		padding: 0 16px;
		text-decoration: none;
	}
	
	header .header_customer_service {
	    color: #6EBB2B;
	    font-size: 14px;
	    font-family:'AkzidenzGrotesk-Bold';
	    line-height: 17px;
	    margin-right: 0;
	}
	
	header .switches {
	    background: #FFFFFF;
	    float: right;
	    font-size: 12px;
	    padding: 0 0 1px;
	}
	header .switches .lang-sep {
		border-right: 1px solid #D0D0D0;
	    float: left;
	    height: 16px;
	    margin: 1px 8px;
	}
	.switch strong.current { font-weight: normal; float: left; }
	
	header .switches .switch span {
	  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";	/* IE 8 */
	  filter: alpha(opacity=50);	  /* IE 5-7 */
	  -moz-opacity: 0.5;	  /* Netscape */
	  -khtml-opacity: 0.5;	  /* Safari 1.x */
	  opacity: 0.5;	  /* Good browsers */
	  float: left;
	}
	header .switches .switch span.language-test_spanish { display: none; }
	
	header .switches .switch span:hover {
	  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";	/* IE 8 */
	  filter: alpha(opacity=100);	  /* IE 5-7 */
	  -moz-opacity: 1;	  /* Netscape */
	  -khtml-opacity: 1;	  /* Safari 1.x */
	  opacity: 1;	  /* Good browsers */
	}
	
	header .switches .switch span a { color: #777; }
	header .switches .switch span:hover a { color: #000; }
	
	.switch {
	  display: inline;
	  float: left;
	  line-height: 16px;
	  margin-right: 0;
	  padding: 0 24px;
	}
/*================================================================================ */
/* 4. Navigation														Magify.com
/*================================================================================ */

	nav.primary{ 
		float: left;
	}
	
	nav.primary ul,
	nav.primary ul li {
		margin: 0px;
	}
	
	nav.primary ul li {	
		float: left;
		display: inline;
		height: 33px;
		position: relative;
		z-index: 1;
	}
	
	nav.primary ul {
		border-right: 1px solid #B7B8B8;
		float: left;
	}
	nav.primary ul ul { border-left: 0; }
	
	nav.primary ul li a {
		font-family: "AkzidenzGrotesk-Bold";
		border-left: 1px solid #B7B8B8;
		color: #565657;
		display: inline-block;
		font-size: 13px;
		line-height: 18px;
		padding: 8px 20px;
		text-decoration: none;
		text-shadow: 0 0 2px #FFFFFF;
	}
	nav.primary ul li.level0 a.level-top:hover {
		border-left: 0;
		padding-left: 21px;
	}

	nav.primary ul li.level0:hover + li a {
		border-left: 0;
		margin-left: 1px;
	}
	
	nav.primary ul li.first a { margin-left: 0;}
	
	nav.primary ul li a:hover {
		color: #fe4b05;
		cursor: pointer;
	}

	nav.social-links {
		margin-right: 32px;
	}

	/*sub menu*/
	nav.primary ul li:hover ul.level0 {
		left: -1px;
	}
	
	nav.primary ul ul {
		background: #FFF;
		border-style: none solid solid;
		border-width: 0 1px 1px;
		border-color: #d0d0d0;
		padding: 0px 0px 0px;
		position: absolute;
		top: 33px;
		width: 15.5em;

		left: -9999em;
		z-index: 999999;

		min-width: 100%;
		
		-webkit-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
	    -moz-box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
	    box-shadow: 0px 2px 2px rgba(0, 0, 0, 0.3);
	    
	    border-left: 1px solid #B7B8B8;
	}

	nav.primary ul li:hover ul.level0 li:hover ul.level1 {
		left: 95%;
	}

	nav.primary ul li ul.level1 li:hover ul.level2 {
		left: 95%;
	}

	nav.primary ul li ul ul { 
		left: -999em;
		top: 0; 
	}

	nav.primary ul li.level0:hover {
		left: 0px;
	}
	nav.primary ul li:hover {
		z-index: 999;
		left: -1px;
	}

	nav.primary ul li:hover ul > ul {	
		left: 100%;
	}
	
	nav.primary ul ul li {
		border-bottom: 1px solid #E2E2E2;
		display: list-item;
		float: none;
		height: auto;
		padding: 0px 16px;
	}
	
	nav.primary ul ul li a {
		border: 0; 
		color: #444444;
		display: block;
		font-size: 12px;
		font-weight: normal;
		font-family: 'AkzidenzGrotesk-Regular';
		line-height: 16px;
		padding: 5px 4px;
		position: relative;
	    text-decoration: none !important;
		text-transform: none;
		z-index: 999999;
	}
	
	nav.primary ul ul li:hover,
	nav.primary ul li:hover > a {
		color: #fff;
		background: #F16226;
		text-shadow: none;
	}
	nav.primary ul li.level0:hover > a {
		background: #fff;
		color: #F16226;
		border-radius: 6px 6px 0 0;
		-webkit-box-shadow: 0 -1px 3px rgba(0,0,0,.3);
		box-shadow: 0 -1px 3px rgba(0,0,0,.3);
	}

	nav.primary ul li.level0.active > a {
		background: #f3f3f3;
		color: #F16226;
	}	
	/* nav.primary ul ul ul { left: 100%; } */
	
	nav.user{
	
	}
	nav.user ul,
	nav.userul li {
		margin: 0px;
	}
	
	nav.user ul li {
		display: inline;
		float: right;
		text-align: right;
		position: relative;
		padding-left:25px;
		margin: 0;
	}
	
	nav.user ul li a {
		display: inline-block;
		line-height: 18px;
		color: #121212;
		text-decoration: none;
		text-transform: uppercase;
		font-size: 11px;
	}
	
	nav.user ul li.topcart {
		margin: 12px 0 0 0;
	}
	nav.user ul li.topcart .block-cart {
	}

	nav.user ul li.topcart .block-cart .block-title {
		background: #999da1; /* Old browsers */
		background: -moz-linear-gradient(top,  #999da1 0%, #969a9e 50%, #6d7073 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#999da1), color-stop(50%,#969a9e), color-stop(100%,#6d7073)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #999da1 0%,#969a9e 50%,#6d7073 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  #999da1 0%,#969a9e 50%,#6d7073 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  #999da1 0%,#969a9e 50%,#6d7073 100%); /* IE10+ */
		background: linear-gradient(to bottom,  #999da1 0%,#969a9e 50%,#6d7073 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#999da1', endColorstr='#6d7073',GradientType=0 ); /* IE6-9 */
		border-radius: 3px; -webkit-border-radius: 3px;
		float: left;
		line-height: 14px;
		padding: 6px 16px 7px;
	}
	nav.user ul li.topcart .block-cart .block-title strong {
		font-weight: normal;
		color: #fff; 
	}
	
	nav.user ul li.topcart .block-cart .block-content {
	}
		
	nav.user ul li.topcart .block-cart .block-content .summary {
		clear: both;
		float: right;
		margin: 8px 0 0 0;
	}

	nav.user ul li.topcart .block-cart .block-content .summary .amount{
	}

	nav.user ul li.topcart .block-cart .block-content .summary .subtotal{
	}
	nav.user ul li.topcart .block-cart .block-content .summary .subtotal .label{ color: #6ebb2b; font-size: 12px; margin-right: 24px; }
	nav.user ul li.topcart .block-cart .block-content .summary .subtotal .price{ 
		font-size: 16px; color: #636466;
		font-family: 'AkzidenzGrotesk-Bold';
	}


	nav.user ul li.topcart .block-cart .block-content .actions {
		clear: both;
		float: right;
		margin: 16px 0 0;	
	}

	nav.user ul li.topcart .block-cart .block-content .actions button.button{
		background: #e75c32; /* Old browsers */
		background: -moz-linear-gradient(top,  #e75c32 0%, #e05930 50%, #ae4525 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e75c32), color-stop(50%,#e05930), color-stop(100%,#ae4525)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(top,  #e75c32 0%,#e05930 50%,#ae4525 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(top,  #e75c32 0%,#e05930 50%,#ae4525 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(top,  #e75c32 0%,#e05930 50%,#ae4525 100%); /* IE10+ */
		background: linear-gradient(to bottom,  #e75c32 0%,#e05930 50%,#ae4525 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e75c32', endColorstr='#ae4525',GradientType=0 ); /* IE6-9 */
		border-radius: 5px;
		font-size: 16px;
		font-weight: normal;
		height: 38px;
		padding: 4px 38px 4px 32px;
		text-shadow: 0 0 3px rgba(0, 0, 0, 0.5);
		text-transform: capitalize;
	}
	
	nav.user ul li.topcart .block-cart .block-content .mini-products-list {
		border-top: 2px solid #D0D0D0;
		clear: both;
		float: left;
		margin: 0;
		width: 100%;
	}
	
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li {
		clear: both;
		float: left;
		margin: 8px 0 0 0;
		padding: 0 0 8px;
		width: 100%;
		border-bottom: 1px solid #d0d0d0;
	}
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-image{ float: left; border: 1px solid #d0d0d0; }
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details{
		float: left;
		margin-left: 16px;
		max-width: 312px;
		text-align: left;
    }
	
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .btn-remove { position: absolute; right: 0; color: #000; top: 0; }
	
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .product-name { margin-right: 16px; }
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .product-name a { color: #444; }
	
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .price {}
	
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .truncated {}
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .truncated .item-options dt { float: left; font-style: italic; margin-right: 8px; }
	nav.user ul li.topcart .block-cart .block-content .mini-products-list li .product-details .truncated .item-options dd { float: left; margin-right: 8px; }
	
		
	nav.user ul li.topcart a,
	nav.user ul li.topcart a:hover {  color: #FFFFFF; font-family: "AkzidenzGrotesk-Bold"; }
		
	nav.user ul li.first a { margin-left: 0;}
	
	nav.user ul li a:hover {
		color: #fe4b05;
		cursor: pointer;
	}

