/* 5. Catalogsearch													Magify.com
/*================================================================================ */

	.topsearch{
		height: 30px;
		margin: 12px 0;
	}
	.topsearch label{ display: none; }
	
	.topsearch button {
		background: url("../images/icons.png") no-repeat scroll right -144px transparent;
		border: 0 none;
		color: transparent;
		float: right;
		height: 30px;
		margin: 0 0 0 4px;
		padding: 0;
		text-shadow: none;
		width: 30px;
	}
		
	}
	.topsearch button:hover{
		background: url(../images/search.png);
		cursor: pointer;
		border: 0;
	}
	.topsearch .input-text {
		border: 1px solid #C7C7C7;
		border-radius: 2px;
		color: #004FAE;
		float: left;
		font-size: 12px;
		font-style: italic;
		height: 30px;
		line-height: 12px;
		margin: 0 0 0 10px;
		padding: 4px 12px;
		text-align: right;
		width: 190px;
	}

