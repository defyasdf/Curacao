/* 5. Footer														Magify.com
/*================================================================================ */

	footer{
		background: url("../images/background_footer.png") repeat-x scroll 74px 0 #D25B21;
		border-top: 4px double #FFFFFF;
		margin: 16px auto 0;
		padding: 16px 0;
		width: 100%;
	}
	footer address {
		color: #FFFFFF;
		font-size: 12px;
		font-weight: normal !important;
	}
	footer .mini-logo {
		float: left;
		position: relative; 
	}
	footer .mini-logo h3 {
		background: url("../images/mini-logo.png") no-repeat scroll 0 0 transparent;
		line-height: 32px;
		margin-bottom: 16px;
		padding: 26px 0 0 140px;
		vertical-align: bottom;
		width: auto;
	}
	footer h3 {
		color: #fff;
		float: none;
		font-size: 14px;
		margin-bottom: 2px;
		white-space: nowrap;
	}

	footer .columns.four { 
		margin-bottom: 32px; 
	}
	footer .columns.four.social { border: 0; min-height: 0; width: 196px; }
	
	ul.social-icons li { float: left; margin:0 8px 0 0; }
	ul.social-icons li span {
		height: 24px;
		display: block;
		text-indent: -999em;
		width: 25px;
		margin: 3px 0 0 0;
	}
	
	.credit-ssl-badge { margin: 16px 0 8px; }
	
	ul.social-icons li.facebook span 		{ background: url("../images/social-icons.png") repeat scroll 0 -30px transparent; width: 18px; }
	ul.social-icons li.twitter span 		{ background: url("../images/social-icons.png") repeat scroll -32px -30px transparent; }
	ul.social-icons li.gplus span 			{ background: url("../images/social-icons.png") repeat scroll -72px -30px transparent; }
	
	header ul.social-icons li.facebook span { background: url("../images/social-icons.png") repeat scroll 0 0 transparent; }
	header ul.social-icons li.twitter span 	{ background: url("../images/social-icons.png") repeat scroll -32px 0 transparent; }
	header ul.social-icons li.gplus span 	{ background: url("../images/social-icons.png") repeat scroll -72px 0 transparent; }
	
	footer h3.column-title {}
	
	footer ul,
	footer ul.links {
		display: block;
		float: left;
		line-height: 16px;
		margin: 0;
		padding: 0;
		width: 100%;
	}
	footer ul.links li,
	footer ul li {
		display: block;
		margin: 0 0 8px;
		padding: 0;
		vertical-align: top;
	}
	
	footer a,
	footer a:visited { color: #fff; }


	footer li{
		font-size: 11px;
		line-height: 14px;
		margin: 0;
	}
	footer .contact ul.first{ float: left; }
	footer .contact ul.second{ float: right; }
	
	footer .corporate ul { width: 50%; }

	footer .form-subscribe { margin: 24px 0 0 0; }	
	footer .form-subscribe .form-subscribe-header { float: left; margin-right: 8px; }

	label, legend {  font-weight: normal;}

	footer .form-subscribe .form-subscribe-header label {
		color: #FFFFFF;
		font-size: 14px;
		font-family: 'AkzidenzGrotesk-Bold';
		line-height: 32px; 
		text-decoration: none;
		text-transform: uppercase;
	}

	footer .form-subscribe  button{
		background: url("../images/icons.png") no-repeat scroll 0 -144px transparent;
		border-radius: 0 5px 5px 0;
		float: right;
		height: 30px;
		margin: 0;
		padding: 0;
		width: 30px;
	}
	.form-subscribe button.button span {
		text-indent: -999em;
		display: none;
	}

	footer .form-subscribe .input-text{
		border-radius: 4px;
		float: left;
		font-style: italic;
		margin: 0 0 0 2px;
		padding: 6px 8px;
		text-align: right;
		width: 172px;
	}	
	footer .form-subscribe .input-text:focus{
